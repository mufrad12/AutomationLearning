{
    "name": "auxo-ael-shams",
    "version": "1.0.0",
    "description": "Test Automation framework using playwright and typescript",
    "main": "index.js",
    "scripts": {
        "lint": "eslint .",
        "format": "prettier --write .",
        "check-unused": "ts-node src/utilities/house_keeping/find-unused.ts",
        "suk-generate:report": "allure generate ./test-reports/allure-result --clean -o test-reports/allure-report",
        "allure-generate": "allure generate artifacts/test_reports/allure-results --clean -o artifacts/test_reports/allure-report",
        "allure-open": "allure open artifacts/test_reports/allure-report",
        "allure-report": "npm run allure:generate && npm run allure:open",
        "open:report": "allure open ./test-reports/allure-report",
        "test:stg:chrome": "cross-env TEST_ENV=staging BROWSER=DesktopChrome npx artifacts test --workers=4",
        "test:stg:firefox": "cross-env TEST_ENV=staging BROWSER=DesktopFirefox npx artifacts test",
        "test:qa:chrome": "cross-env TEST_ENV=qa BROWSER=DesktopChrome npx artifacts test",
        "test:qa:firefox": "cross-env TEST_ENV=qa BROWSER=DesktopFirefox npx artifacts test",
        "test:stg:chrome:report": "cross-env TEST_ENV=staging BROWSER=DesktopChrome npx artifacts test && npm run generate:report",
        "check-all": "node src/utilities/house_keeping/check-all.js",
        "git-stats": "ts-node src/utilities/reporters/commit_parser.ts",
        "clear-sessions": "npx playwright test --grep 'Delete all JSON files in artifacts/auth and src/sessions'",
        "postinstall": "npx playwright install --with-deps"
    },
    "repository": {
        "type": "git",
        "url": "ratul.com"
    },
    "author": "Syed Shams Elahi",
    "license": "ISC",
    "bugs": {
        "url": "https://www.ratul.com/bugs"
    },
    "homepage": "ratul.com/home",
    "devDependencies": {
        "@eslint/js": "9.14.0",
        "@playwright/mcp": "0.0.29",
        "@playwright/test": "1.53.0",
        "@typescript-eslint/eslint-plugin": "7.18.0",
        "@typescript-eslint/parser": "7.18.0",
        "eslint": "8.57.0",
        "eslint-config-prettier": "9.1.0",
        "eslint-plugin-jsonc": "2.20.0",
        "eslint-plugin-playwright": "1.8.3",
        "eslint-plugin-prettier": "5.2.1",
        "eslint-plugin-unused-imports": "4.1.4",
        "eslint-plugin-yml": "1.18.0",
        "fast-glob": "3.3.3",
        "jsonc-eslint-parser": "2.4.0",
        "playwright-ci-reporter": "1.5.3",
        "prettier": "3.3.3",
        "ts-morph": "25.0.1",
        "ts-unused-exports": "11.0.1",
        "yaml-eslint-parser": "1.3.0"
    },
    "dependencies": {
        "@faker-js/faker": "^9.9.0",
        "@types/fs-extra": "11.0.4",
        "@types/node": "22.7.1",
        "allure-commandline": "2.32.0",
        "allure-patch": "1.0.3",
        "allure-playwright": "3.0.8",
        "cross-env": "7.0.3",
        "dotenv": "16.4.5",
        "error-stack-parser": "2.1.4",
        "globals": "15.12.0",
        "left-pad": "1.3.0",
        "monocart-reporter": "2.9.13",
        "playwright-html-reporter": "0.1.11",
        "ts-node": "10.9.2",
        "typescript": "5.4.5"
    }
}
